@model UserDtoForInsertion

<div class="TamSayfaCerceveAlaniIciAltiCizgiliBaslikAlaniUser">
                <div class="TamSayfaCerceveAlaniIciAltiCizgiliBaslikAlaniUstBaslikMetniUser">
                    Ekleme
                </div>
            </div>
<form class="UyelikBilgileriUser" method="post" asp-action="Update">
                <div class="text-danger" asp-validation-summary="All"></div>
                @*<input type="hidden" asp-for="Id" />*@
                <input  type="hidden" asp-for="Password"/>
                <div class="TamSayfaCerceveAlaniIciFormElemanlariBaslikMetniUser"><label >T.C Kimlik No</label></div>
                <div class="TamSayfaCerceveAlaniIciFormElemanlariIcinSatirAlaniUser"><input type="text" class="FormElemanlariTextInputlari" /></div>
                <div class="TamSayfaCerceveAlaniIciFormElemanlariBaslikMetniUser"><label >Ad</label></div>
                <div class="TamSayfaCerceveAlaniIciFormElemanlariIcinSatirAlaniUser"><input type="text" class="FormElemanlariTextInputlari" /></div>
                <div class="TamSayfaCerceveAlaniIciFormElemanlariBaslikMetniUser"><label >Soyad</label></div>
                <div class="TamSayfaCerceveAlaniIciFormElemanlariIcinSatirAlaniUser"><input type="text" class="FormElemanlariTextInputlari" /></div>
                <div class="TamSayfaCerceveAlaniIciFormElemanlariBaslikMetniUser"><label >E-Mail</label></div>
                <div class="TamSayfaCerceveAlaniIciFormElemanlariIcinSatirAlaniUser"><input type="email" class="FormElemanlariTextInputlari" /></div>
                <div class="TamSayfaCerceveAlaniIciFormElemanlariBaslikMetniUser"><label >Telefon No</label></div>
                <div class="TamSayfaCerceveAlaniIciFormElemanlariIcinSatirAlaniUser"><input type="text" class="FormElemanlariTextInputlari" /></div>


                <div class="TamSayfaCerceveAlaniIciFormElemanlariBaslikMetniUser">Rolünü Seç</div>
                @foreach(var role in Model.Roles)
    {
        <div class="CheckBoxlarveSecenekleriIcinKapsamaAlani">
							<span class="CheckBoxveSeceneklerKapsamaAlani">
								<label class="CheckBoxKapsamaAlani">								
									<input name="SmsIleHeberdarOl" type="checkbox" class="FormElemanlariCheckBoxInputlari">
									<span class="CheckBoxIcinBicimlendirmeAlani"></span>
									<span class="CheckBoxYaniMetinAlani">@role</span>
								</label>
							</span>
						</div>
    }
    
                <div style="margin-top:13px;" class="ButonlarIcinKapsamaAlani"><input type="submit" value="Ekle" class="FormElemanlariYesilButonlar" tabindex="6"></div>
</form>
